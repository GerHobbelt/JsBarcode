{
  "name": "io-barcode",
  "version": "1.0.0",
  "description": "Isomorphic barcode generator for Node and browsers",
  "keywords": [
    "barcode",
    "generator",
    "isomorphic",
    "canvas",
    "UPC",
    "CODE128",
    "CODE39",
    "Pharmacode",
    "ITF14"
  ],
  "main": "./build/npm",
  "scripts": {
    "test": "babel-node test/server-test.js && webpack-dev-server index.js",
    "build-browser": "rm -rf build/browser && webpack modules/index.js build/browser/jsbarcode.js && NODE_ENV=production webpack modules/index.js build/browser/jsbarcode.min.js && echo \"gzipped, the global build is `gzip -c build/browser/jsbarcode.min.js | wc -c` bytes\"",
    "build-npm": "rm -rf build/npm && babel -d build/npm ./modules",
    "lint": "eslint index.js encodings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavded/JsBarcode.git"
  },
  "keywords": [],
  "author": "Marc Harter <wavded@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wavded/JsBarcode/issues"
  },
  "homepage": "https://github.com/wavded/JsBarcode#readme",
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-loader": "^5.0.0",
    "eslint": "^0.19.0",
    "webpack": "^1.8.9",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "canvas-browserify": "^1.1.1"
  }
}
